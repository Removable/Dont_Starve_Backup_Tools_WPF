                                    <Setter Property="Width" TargetName="columnDefinition" Value="Auto"/>
                                    <Setter Property="MinWidth" TargetName="columnDefinition" Value="0"/>
                                    <Setter Property="Width" TargetName="columnDefinition1" Value="*"/>
